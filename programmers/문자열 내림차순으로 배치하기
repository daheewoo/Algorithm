import java.util.*;

class Solution {
  public String solution(String s) {
      String answer = "";
      String[] Arr = s.split(""); //String클래스의 split메서드를 이용해서 각 문자 배열로만들기
      
      Arrays.sort(chArr);         //sort메서드를 이용하여 오름차순으로 정렬
      
      for(int i=chArr.length-1; i>=0; i--) {
	    	answer += chArr[i];       //for문을 이용하여 내림차순으로 단어 
	  }
      
      return answer;
  }
}
